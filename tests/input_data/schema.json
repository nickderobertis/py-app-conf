    "default_uuid_list": {
      "title": "Default Uuid List",
      "env_names": [
        "myapp_default_uuid_list"
      ],
      "type": "array",
      "items": {
        "type": "string",
        "format": "uuid"
      }
    },
    "file_path": {
      "title": "File Path",
      "default": "/a/b.txt",
      "env_names": [
        "myapp_file_path"
      ],
      "type": "string",
      "format": "path"
