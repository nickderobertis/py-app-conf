      "default": "woo",
      "env_names": [
        "myapp_default_string"
      ],
      "type": "string"
    },
    "default_custom": {
      "title": "Default Custom",
      "default": {
        "a": "yeah",
        "b": 5.6
      },
      "env_names": [
        "myapp_default_custom"
      ],
      "allOf": [
        {
          "$ref": "#/definitions/SubModel"
        }
      ]
